package emag;

public class Admin extends User {

	private int adminId;

	public Admin(int id, String name, String email, String password) {
		super(name, email, password);
		this.setId(id);
	}
	
	public void setId(int id) {
		this.adminId=id;
	}
	
	public int getAdminId() {
		return adminId;
	}
	
	public String getName(){
		return getName();
	}

}
